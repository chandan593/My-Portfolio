    <%- include('partials/navbar'); %>
    <div class="contactUs">
        <div class="title">
            <h2>Get In Touch</h2>
        </div>
        <div class="box">
            <!-- Form Box -->
            <div class="contact form">
                <h3>Send A Message</h3>
                <form action="https://formsubmit.co/chandanshingumofficial@gmail.com" method="post">
                    <div class="formBox">
                        <div class="row50">
                            <div class="inputBox">
                                <span>First Name</span>
                                <input type="text" name="fname" id="" placeholder="Enter Your First Name....">
                            </div>
                            <div class="inputBox">
                                <span>Last Name</span>
                                <input type="text" name="lname" id="" placeholder="Enter Your Last Name....">
                            </div>
                        </div>

                        <div class="row50">
                            <div class="inputBox">
                                <span>Email Address</span>
                                <input type="email" name="email" id="" placeholder="Enter Your Email....">
                            </div>
                            <div class="inputBox">
                                <span>Mobile</span>
                                <input type="tel" name="phone" id="" placeholder="" maxlength="10">
                            </div>
                        </div>

                        <div class="row100">
                            <div class="inputBox">
                                <span>Message</span>
                                <textarea name="message" id="" cols="30" rows="10"
                                    placeholder="Enter Your Message Here...."></textarea>
                            </div>
                        </div>

                        <div class="row100">
                            <div class="inputBox">
                                <input type="submit" value="Submit">
                            </div>
                        </div>
                    </div>
                </form>
            </div>

            <!-- Info Box -->
            <div class="contact info">
                <h3>Contact Info</h3>
                <div class="infoBox">
                    <div>
                        <span>
                            <ion-icon name="location"></ion-icon>
                        </span>
                        <p>Savant Vihar Phase 1, Katraj, Pune <br>INDIA</p>
                    </div>
                    <div>
                        <span>
                            <ion-icon name="mail"></ion-icon>
                        </span>
                        <a href="mailto:chandanshingumofficial@gmail.com">chandanshingumofficial@gmail.com</a>
                    </div>
                    <div>
                        <span>
                            <ion-icon name="call"></ion-icon>
                        </span>
                        <a href="tel:+919470678687">+91 94706 78687</a>
                    </div>
                    <!-- Social Media Icons -->
                    <div class="sci">
                        <li><a href="https://github.com/chandan593" target="_blank">
                                <ion-icon name="logo-github"></ion-icon>
                            </a></li>
                        <li><a href="https://www.instagram.com/chandann_011/" target="_blank">
                                <ion-icon name="logo-instagram"></ion-icon>
                            </a></li>
                        <li><a href="https://twitter.com/Chandan31929656" target="_blank">
                                <ion-icon name="logo-twitter"></ion-icon>
                            </a></li>
                        <li><a href="https://www.linkedin.com/in/chandan-kumar-b650b11b8/" target="_blank">
                                <ion-icon name="logo-linkedin"></ion-icon>
                            </a></li>
                    </div>
                </div>
            </div>

            <!-- Map Box -->
            <div class="contact map">
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3784.7025539402835!2d73.85447587389596!3d18.451809571273547!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3bc2eac44feaaaab%3A0xc3217eba313adf07!2sSawant%20Vihar%20Phase%3A-%201!5e0!3m2!1sen!2sin!4v1692047087714!5m2!1sen!2sin" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade">
                </iframe>
            </div>
        </div>
